{
  "name": "alexa-givennames-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "build": "npm-run-all --parallel build:skill",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy --stage dev",
    "convert-assets": "rsvg-convert -w 108 -o assets/icon108.png assets/icon.svg && rsvg-convert -w 512 -o assets/icon512.png assets/icon.svg",
    "create-s3-bucket": "aws s3 ls s3://alexa-givennames-skill > /dev/null 2>&1 || aws s3 mb s3://alexa-givennames-skill --region eu-west-1",
    "deploy:assets": "aws s3 sync ./assets s3://alexa-givennames-skill/ --acl public-read"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "^9.2.0",
    "alexa-request": "^0.3.4",
    "alexa-response": "^4.2.10",
    "alexa-utterances": "^0.2.1",
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^4.1.2",
    "chai-subset": "^1.6.0",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^4.13.1",
    "node-aws-lambda": "^0.1.8",
    "npm-run-all": "^4.1.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "serverless": "^1.24.1",
    "webpack": "^3.10.0",
    "webpack-sources": "1.0.1"
  },
  "files": [
    "build",
    "src"
  ],
  "keywords": [
    "alexa",
    "alexa-skill",
    "echo",
    "skill"
  ]
}
